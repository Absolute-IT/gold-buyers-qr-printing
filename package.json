{
  "name": "qr-code-printing",
  "version": "1.0.0",
  "description": "Test QR code printing for Brother QL-700 label printer.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src --config eslint.config.js",
    "fix": "eslint src --fix --config eslint.config.js",
    "build": "tsc -p .",
    "start": "pnpm build && node ./dist/index.js",
    "start:dev": "pnpm build && DEBUG=* node ./dist/index.js",
    "start:polling": "pnpm build && node ./dist/index.js",
    "prepare": "pnpm build"
  },
  "imports": {
    "#classes/*": [
      "./dist/classes/*.js",
      "./src/classes/*.ts"
    ],
    "#constants/*": [
      "./dist/constants/*.js",
      "./src/constants/*.ts"
    ],
    "#types/*": [
      "./dist/types/*.js",
      "./src/types/*.d.ts"
    ],
    "#services/*": [
      "./dist/services/*.js",
      "./src/services/*.ts"
    ]
  },
  "keywords": [],
  "author": "Matthew Scott",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "@types/qrcode": "^1.5.5",
    "eslint": "^9.33.0",
    "eslint-config-love": "^121.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "dependencies": {
    "canvas": "^3.2.0",
    "dotenv": "^17.2.1",
    "node-brother-label-printer": "github:Zei33/node-brother-label-printer",
    "qrcode-esm": "^1.5.1",
    "uuid": "^11.1.0"
  }
}
